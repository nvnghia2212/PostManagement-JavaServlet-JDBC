<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">

	<!--excludes không được apply giao diện -->
	<excludes>
		<pattern>/api*</pattern>
	</excludes>

	<!--decorator được apply giao diện -->
	<decorator name="admin" page="admin.jsp">
		<pattern>/admin*</pattern>
	</decorator>

	<decorator name="user" page="user.jsp">
		<pattern>/*</pattern>
	</decorator>

	<decorator name="login" page="login.jsp">
		<pattern>/login</pattern>
	</decorator>

</decorators>